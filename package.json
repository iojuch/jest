{
  "jest": {
        "collectCoverageFrom": [
            "**/*.{js,jsx}",
            "!**/node_modules/**",
            "!**/coverage/**"
        ],
        "coverageThreshold": {
            "global": {
                "branches": 100,
                "functions": 100,
                "lines": 100
            }
        }
    },
    "version": "1.0.0",
    "description": "jest test",
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "coverage": "jest --collectCoverage",
        "test:watch": "jest --watchAll"
    },
    "keywords": [
        "jest",
        "test",
        "jsaqa"
    ],
  "devDependencies": {
    "@eslint/js": "^9.8.0",
    "eslint": "^9.8.0",
    "eslint-config-rallycoding": "^3.2.0",
    "eslint-plugin-react": "^7.35.0",
    "globals": "^15.9.0",
    "jest": "^29.7.0"
  }
}
